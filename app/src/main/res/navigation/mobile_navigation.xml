<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/landingFragment2">


    <fragment
        android:id="@+id/finfestAndLitfestFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.EventFinfestAndLitfestFragment"
        android:label="fragment_finfest_and_litfest"
        tools:layout="@layout/fragment_event_finfest_and_litfest" />

    <fragment
        android:id="@+id/speceficEventFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.SpecificEventFragment"
        android:label="fragment_specefic_event"
        tools:layout="@layout/fragment_specific_event" />

    <fragment
        android:id="@+id/workshopsFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.WorkshopsFragment"
        android:label="fragment_workshops"
        tools:layout="@layout/fragment_workshops" />

    <fragment
        android:id="@+id/landingFragment2"
        android:name="com.mdgiitr.karthik.cognizance19.view.LandingFragment"
        android:label="fragment_landing"
        tools:layout="@layout/fragment_landing">
        <action
            android:id="@+id/action_landingFragment2_to_userLoginFragment"
            app:destination="@id/userLoginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_landingFragment2_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/userLoginFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.UserLoginFragment"
        android:label="fragment_user_login"
        tools:layout="@layout/fragment_user_login">
        <action
            android:id="@+id/action_userLoginFragment_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.OnBoardingFragment"
        android:label="onboarding_fragment"
        tools:layout="@layout/fragment_onboarding">
        <action
            android:id="@+id/action_onBoardingFragment_to_completeYourProfileFragment"
            app:destination="@id/completeYourProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_onBoardingFragment_to_myProfileFragment"
            app:destination="@id/myProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/completeYourProfileFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.CompleteYourProfileFragment"
        android:label="fragment_complete_your_profile"
        tools:layout="@layout/fragment_complete_your_profile" />
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.mdgiitr.karthik.cognizance19.view.MyProfileFragment"
        android:label="my_profile"
        tools:layout="@layout/my_profile">
        <action
            android:id="@+id/action_myProfileFragment_to_landingFragment2"
            app:destination="@id/landingFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
</navigation>