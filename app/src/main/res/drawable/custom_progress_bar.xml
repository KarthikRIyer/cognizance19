<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/progress_background">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <solid android:color="#ff78849e" />
        </shape>
    </item>

    <item android:id="@+id/progress_foreground">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="4dp" />
                <solid android:color="#ff00aeef" />
            </shape>
        </clip>
    </item>

</layer-list>